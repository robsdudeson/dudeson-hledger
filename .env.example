# Example .env file for hledger project
# Copy this to .env and fill in your actual values
# DO NOT commit .env to version control!

# SimpleFIN Configuration
# Get your Setup Token from: https://beta-bridge.simplefin.org/
# After running bin/simplefinsetup, credentials are stored in ~/.simplefin/
# SIMPLEFIN_ACCESS_URL=https://...

# Custom script configuration
# PROJECT_ROOT=/home/user/code/dudeson-hledger
# DEFAULT_ACCOUNT=Assets:Checking:Chase

# Categorization settings
# AUTO_CATEGORIZE=true
# CONFIDENCE_THRESHOLD=0.9

# Notification settings (optional)
# NOTIFY_ON_SYNC=false
# NOTIFY_EMAIL=your@email.com

SIMPLEFIN_SETUP_TOKEN=""
SIMPLEFIN_ACCESS_URL=""
